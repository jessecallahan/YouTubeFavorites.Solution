@{
  Layout = "_Layout";
}

@using YouTubeFavorites.Models;

<h3>Here are all the pages in this category:</h3>

<ol>
  @foreach (YouTubePage youtubepage in @Model["youtubepages"])
  {
    <li><a href='/categories/@Model["category"].Id/youtubepages/@youtubepage.Id'>@youtubepage.Title</a></li>
  }
</ol>

<p><a href='/categories/@Model["category"].Id/youtubepages/new'>Add a new youtubepage</a></p>
<p><a href='/categories'>Return to category list</a></p>
<p><a href='/'>Return to Main youtubepage</a></p>